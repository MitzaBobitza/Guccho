{
  "name": "guccho",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "build": "yarn build:schemas && yarn build:gen && nuxt build",
    "build:schemas": "node scripts/build-schema.mjs",
    "build:gen": "node scripts/gen-hljs.mjs",
    "dev": "nuxt --dotenv .env.development dev",
    "generate": "nuxt generate",
    "preview:dev": "nuxt --dotenv .env.development preview",
    "preview:prod": "nuxt preview",
    "start:prod": "dotenv -e .env node .output/server/index.mjs",
    "start:dev:inspect": "dotenv -e .env.development node -- --inspect .output/server/index.mjs",
    "analyze": "nuxt analyze",
    "postinstall": "yarn build:schemas && nuxt prepare && yarn build:gen",
    "lint": "eslint --ext '.js,.vue,.ts,.jsx,.tsx' --ignore-path .gitignore .",
    "lint:fix": "yarn lint --fix",
    "lint:type-check": "tsc -p . --noEmit && vue-tsc --noEmit",
    "sync-from-lock": "yarn upgrade && npx syncyarnlock --save --keepPrefix && yarn install",
    "check-update": "npx npm-check-updates",
    "update-dep": "npx npm-check-updates -u && yarn install"
  },
