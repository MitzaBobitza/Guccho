{
  "extends": "../../.nuxt/tsconfig.server.json",
  "include": ["./**/*"],
  "compilerOptions": {
    "baseUrl": "..",
    "paths": {
      "~": ["../src"],
      "~/*": ["../src/*"],
      "@": ["../src"],
      "@/*": ["../src/*"],
      "~~": [".."],
      "~~/*": ["../*"],
      "@@": [".."],
      "@@/*": ["../*"],
      "$active": ["../src/server/backend/ppy.sb@bancho.py"],
      "$active/*": ["../src/server/backend/ppy.sb@bancho.py/*"],
      "$base": ["../src/server/backend/$base"],
      "$base/*": ["../src/server/backend/$base/*"],
      "$articles": ["../articles"],
      "$articles/*": ["../articles/*"]
    }
  }
}
